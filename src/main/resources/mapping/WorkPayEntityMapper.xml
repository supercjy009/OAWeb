    <select id="selectAllOrder" resultMap="BaseResultMap" parameterType="demo.model.dto.WorkPayReqVo">
        select
        <include refid="Query_Column_List"/>
        from oa_work o LEFT JOIN user_info r1 ON o.pay_user = r1.uid
        <where>
            <if test="payDate != null">
                <![CDATA[`pay_date` <= DATE_ADD(#{payDate, jdbcType=TIMESTAMP},INTERVAL 1 MONTH ) AND `pay_date` >= #{payDate, jdbcType=TIMESTAMP}]]>
            </if>
            <if test="partName != null and partName !=''">
                and part_name = #{partName,jdbcType=VARCHAR}
            </if>
            <if test="getUser != null and getUser !=''">
                and get_user = #{getUser,jdbcType=VARCHAR}
            </if>
            <if test="audit != null and audit !=''">
                and audit = #{audit,jdbcType=VARCHAR}
            </if>
            <if test="settle != null and settle !=''">
                and settle = #{settle,jdbcType=VARCHAR}
            </if>
        </where>
        order by pay_date desc
    </select>

